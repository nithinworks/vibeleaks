{
  "title": "gitleaks config",
  "allowlist": {
    "description": "global allow lists",
    "regexes": [
      "(?i)^true|false|null$",
      "^(?i:a+|b+|c+|d+|e+|f+|g+|h+|i+|j+|k+|l+|m+|n+|o+|p+|q+|r+|s+|t+|u+|v+|w+|x+|y+|z+|\\*+|\\.+)$",
      "^\\$(?:\\d+|{\\d+})$",
      "^\\$(?:[A-Z_]+|[a-z_]+)$",
      "^\\${(?:[A-Z_]+|[a-z_]+)}$",
      "^\\{\\{[ \\t]*[\\w ().|]+[ \\t]*}}$",
      "^\\$\\{\\{[ \\t]*(?:(?:env|github|secrets|vars)(?:\\.[A-Za-z]\\w+)+[\\w \"'&./=|]*)[ \\t]*}}$",
      "^%(?:[A-Z_]+|[a-z_]+)%$",
      "^%[+\\-# 0]?[bcdeEfFgGoOpqstTUvxX]$",
      "^\\{\\d{0,2}}$",
      "^@(?:[A-Z_]+|[a-z_]+)@$",
      "^/Users/(?i)[a-z0-9]+/[\\w .-/]+$",
      "^/(?:bin|etc|home|opt|tmp|usr|var)/[\\w ./-]+$"
    ],
    "paths": [
      "gitleaks\\.toml",
      "(?i)\\.(?:bmp|gif|jpe?g|png|svg|tiff?)$",
      "(?i)\\.(?:eot|[ot]tf|woff2?)$",
      "(?i)\\.(?:docx?|xlsx?|pdf|bin|socket|vsidx|v2|suo|wsuo|.dll|pdb|exe|gltf|zip)$",
      "go\\.(?:mod|sum|work(?:\\.sum)?)$",
      "(?:^|/)vendor/modules\\.txt$",
      "(?:^|/)node_modules(?:/.*)?$",
      "(?:^|/)(?:deno\\.lock|npm-shrinkwrap\\.json|package-lock\\.json|pnpm-lock\\.yaml|yarn\\.lock)$",
      "(?:^|/)bower_components(?:/.*)?$",
      "(?i)(?:^|/)(?:v?env|virtualenv)/lib(?:64)?(?:/.*)?$",
      "Database.refactorlog"
    ],
    "stopwords": [
      "abcdefghijklmnopqrstuvwxyz",
      "014df517-39d1-4453-b7b3-9930c563627c"
    ]
  },
  "rules": [
    {
      "id": "adafruit-api-key",
      "description": "Identified a potential Adafruit API Key, which could lead to unauthorized access to Adafruit services and sensitive data exposure.",
      "regex": "(?i)[\\w.-]{0,50}?(?:adafruit)(?:[ \\t\\w.-]{0,20})[\\s'\"]{0,3}(?:=|>|:{1,3}=|\\|\\||:|=>|\\?=|,)[\\x60'\"\\s=]{0,5}([a-z0-9_-]{32})(?:[\\x60'\"\\s;]|\\\\[nr]|$)",
      "keywords": ["adafruit"]
    },
    {
      "id": "adobe-client-id",
      "description": "Detected a pattern that resembles an Adobe OAuth Web Client ID, posing a risk of compromised Adobe integrations and data breaches.",
      "regex": "(?i)[\\w.-]{0,50}?(?:adobe)(?:[ \\t\\w.-]{0,20})[\\s'\"]{0,3}(?:=|>|:{1,3}=|\\|\\||:|=>|\\?=|,)[\\x60'\"\\s=]{0,5}([a-f0-9]{32})(?:[\\x60'\"\\s;]|\\\\[nr]|$)",
      "entropy": 2,
      "keywords": ["adobe"]
    },
    {
      "id": "aws-access-key",
      "description": "Identified an AWS Access Key ID, which could lead to unauthorized AWS service access and data exposure.",
      "regex": "(A3T[A-Z0-9]|AKIA|AGPA|AIDA|AROA|AIPA|ANPA|ANVA|ASIA)[A-Z0-9]{16}",
      "keywords": ["akia", "aws"]
    },
    {
      "id": "aws-secret-key",
      "description": "Found a pattern resembling an AWS Secret Access Key, posing a risk of unauthorized AWS resource access.",
      "regex": "(?i)aws[_-]?secret[_-]?access[_-]?key[\\s'\"=:]+([a-z0-9/+=]{40})",
      "keywords": ["aws", "secret"]
    },
    {
      "id": "github-pat",
      "description": "Discovered a GitHub Personal Access Token, potentially allowing unauthorized repository access and modifications.",
      "regex": "ghp_[0-9a-zA-Z]{36}",
      "keywords": ["ghp_"]
    },
    {
      "id": "github-fine-grained-pat",
      "description": "Found a GitHub Fine-Grained Personal Access Token, risking unauthorized access to GitHub resources.",
      "regex": "github_pat_[0-9a-zA-Z_]{82}",
      "keywords": ["github_pat_"]
    },
    {
      "id": "github-oauth",
      "description": "Detected a GitHub OAuth Access Token, which could lead to unauthorized GitHub account access.",
      "regex": "gho_[0-9a-zA-Z]{36}",
      "keywords": ["gho_"]
    },
    {
      "id": "github-app-token",
      "description": "Identified a GitHub App Token, potentially compromising GitHub App integrations and permissions.",
      "regex": "(ghu|ghs)_[0-9a-zA-Z]{36}",
      "keywords": ["ghu_", "ghs_"]
    },
    {
      "id": "github-refresh-token",
      "description": "Discovered a GitHub Refresh Token, which could allow prolonged unauthorized access to GitHub services.",
      "regex": "ghr_[0-9a-zA-Z]{36}",
      "keywords": ["ghr_"]
    },
    {
      "id": "google-api-key",
      "description": "Found a Google API Key, which may lead to unauthorized access to Google services and data exposure.",
      "regex": "AIza[0-9A-Za-z\\-_]{35}",
      "keywords": ["aiza"]
    },
    {
      "id": "google-oauth-access-token",
      "description": "Detected a Google OAuth Access Token, posing a risk of unauthorized access to Google user accounts.",
      "regex": "ya29\\.[0-9A-Za-z\\-_]+",
      "keywords": ["ya29"]
    },
    {
      "id": "heroku-api-key",
      "description": "Uncovered a Heroku API Key, potentially compromising Heroku application deployments and data.",
      "regex": "(?i)[\\w.-]{0,50}?(?:heroku)(?:[ \\t\\w.-]{0,20})[\\s'\"]{0,3}(?:=|>|:{1,3}=|\\|\\||:|=>|\\?=|,)[\\x60'\"\\s=]{0,5}([0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12})(?:[\\x60'\"\\s;]|\\\\[nr]|$)",
      "keywords": ["heroku"]
    },
    {
      "id": "slack-access-token",
      "description": "Found a Slack Access Token, risking unauthorized access to Slack workspaces and data leaks.",
      "regex": "xox[baprs]-([0-9a-zA-Z]{10,48})",
      "keywords": ["xoxb", "xoxa", "xoxp", "xoxr", "xoxs"]
    },
    {
      "id": "slack-webhook-url",
      "description": "Detected a Slack Webhook URL, which could lead to unauthorized message posting and data leakage.",
      "regex": "https://hooks\\.slack\\.com/services/[A-Za-z0-9+/]{43,46}",
      "keywords": ["hooks.slack.com"]
    },
    {
      "id": "stripe-api-key",
      "description": "Identified a Stripe API Key, potentially exposing payment processing services and sensitive financial data.",
      "regex": "(?i)(sk|pk)_(test|live)_[0-9a-z]{24,99}",
      "keywords": ["sk_test", "pk_test", "sk_live", "pk_live"]
    },
    {
      "id": "twilio-api-key",
      "description": "Discovered a Twilio API Key, which could compromise communication services and customer data.",
      "regex": "SK[0-9a-fA-F]{32}",
      "keywords": ["twilio", "SK"]
    },
    {
      "id": "generic-api-key",
      "description": "Detected a Generic API Key, potentially exposing access to various services and sensitive data.",
      "regex": "(?i)(?:api[_-]?key|apikey|access[_-]?key)[\\s'\"=:]+(['\"]?)([a-z0-9_\\-]{20,}|[A-Z0-9_\\-]{20,})\\1",
      "keywords": ["api", "key", "apikey", "access"]
    },
    {
      "id": "password-in-url",
      "description": "Found a password in a URL, risking unauthorized access and credential exposure.",
      "regex": "[a-zA-Z]{3,10}://[^/\\s:@]{3,20}:([^/\\s@]{3,20})@.{1,100}",
      "keywords": ["://"]
    },
    {
      "id": "private-key",
      "description": "Identified a Private Key, which may compromise cryptographic security and sensitive data encryption.",
      "regex": "-----BEGIN[ A-Z]*PRIVATE KEY[ A-Z]*-----",
      "keywords": ["-----BEGIN", "PRIVATE KEY"]
    },
    {
      "id": "rsa-private-key",
      "description": "Found an RSA Private Key, potentially allowing unauthorized cryptographic operations and data decryption.",
      "regex": "-----BEGIN RSA PRIVATE KEY-----",
      "keywords": ["-----BEGIN RSA PRIVATE KEY"]
    },
    {
      "id": "ssh-private-key",
      "description": "Discovered an SSH Private Key, which could enable unauthorized server access and data breaches.",
      "regex": "-----BEGIN OPENSSH PRIVATE KEY-----",
      "keywords": ["-----BEGIN OPENSSH PRIVATE KEY"]
    },
    {
      "id": "pgp-private-key",
      "description": "Detected a PGP Private Key, risking unauthorized access to encrypted communications and data.",
      "regex": "-----BEGIN PGP PRIVATE KEY BLOCK-----",
      "keywords": ["-----BEGIN PGP PRIVATE KEY"]
    },
    {
      "id": "jwt",
      "description": "Identified a JSON Web Token (JWT), which may lead to unauthorized access and identity spoofing.",
      "regex": "eyJ[A-Za-z0-9_-]{10,}\\.[A-Za-z0-9_-]{10,}\\.[A-Za-z0-9_-]{10,}",
      "keywords": ["eyJ"]
    },
    {
      "id": "firebase-url",
      "description": "Found a Firebase URL, potentially exposing Firebase project configurations and data.",
      "regex": ".*\\.firebaseio\\.com",
      "keywords": ["firebaseio.com"]
    },
    {
      "id": "authorization-bearer",
      "description": "Detected an Authorization Bearer Token, risking unauthorized API access and data exposure.",
      "regex": "(?i)authorization[\\s'\"=:]+bearer\\s+([a-z0-9_\\-\\.=]+)",
      "keywords": ["authorization", "bearer"]
    },
    {
      "id": "basic-auth",
      "description": "Found Basic Authentication credentials, potentially exposing user credentials.",
      "regex": "(?i)authorization[\\s'\"=:]+basic\\s+([a-z0-9_\\-\\.=]+)",
      "keywords": ["authorization", "basic"]
    },
    {
      "id": "pkcs12-file",
      "description": "Found a PKCS #12 file, which commonly contain bundled private keys.",
      "regex": "-----BEGIN CERTIFICATE-----",
      "keywords": ["-----BEGIN CERTIFICATE"]
    }
  ]
}
